# Vercel environment example (SAFE PLACEHOLDERS ONLY)

NODE_ENV=production

# Canonical site URL (naked domain - www redirects to this via middleware)
NEXT_PUBLIC_APP_URL=https://levqor.ai

# Stripe (do NOT put real keys here)
STRIPE_SECRET_KEY=YOUR_STRIPE_SECRET_KEY_HERE
STRIPE_WEBHOOK_SECRET=YOUR_STRIPE_WEBHOOK_SECRET_HERE
STRIPE_PUBLISHABLE_KEY=YOUR_STRIPE_PUBLISHABLE_KEY_HERE

# OpenAI (do NOT put real keys here)
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Database
DATABASE_URL=YOUR_DATABASE_CONNECTION_STRING_HERE

# NextAuth / Auth - Must match the canonical domain users sign in on
NEXTAUTH_SECRET=YOUR_NEXTAUTH_SECRET_HERE
NEXTAUTH_URL=https://levqor.ai

# Other service keys (all placeholders only)
SENTRY_DSN=YOUR_SENTRY_DSN_HERE
